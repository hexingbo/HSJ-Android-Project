<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <!--首页底部导航栏的标题，未选中时-->
    <integer-array name="main_tab_un_select" tools:ignore="ResourceName">
        <item name="tab_home">@drawable/main_tab_shop_n</item>
        <item name="tab_find">@drawable/main_tab_business_n</item>
        <item name="tab_data">@drawable/main_tab_cart_n</item>
        <item name="tab_me">@drawable/main_tab_my_n</item>
    </integer-array>

    <!--首页底部导航栏的标题，选中时-->
    <integer-array name="main_tab_select" tools:ignore="ResourceName">
        <item name="tab_home">@drawable/main_tab_shop_s</item>
        <item name="tab_find">@drawable/main_tab_business_s</item>
        <item name="tab_data">@drawable/main_tab_cart_s</item>
        <item name="tab_me">@drawable/main_tab_my_s</item>
    </integer-array>

    <!--首页底部导航栏文字-->
    <string-array name="main_title" tools:ignore="ResourceName">
        <item>逛店铺</item>
        <item>管生意</item>
        <item>购物车</item>
        <item>我的</item>
    </string-array>
</resources>